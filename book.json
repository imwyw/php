{
    "title": "Php",
    "author": "Wang Yuanwei <now_way@126.com>",
    "description": "Php web",
    "language": "zh-hans",
    "gitbook": ">=3.0.0",
    "plugins": [
        "anchors",
        "multipart",
        "-search",
        "-lunr",
        "search-plus",
        "-sharing",
        "splitter",
        "sectionx",
        "anchor-navigation-ex",
        "page-footer-ex",
        "copy-code-button"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        },
        "sectionx": {
            "tag": "b"
        },
        "anchor-navigation-ex": {
            "showLevel": true,
            "associatedWithSummary": true,
            "printLog": false,
            "multipleH1": true,
            "mode": "float",
            "float": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            },
            "pageTop": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        },
        "page-footer-ex": {
            "copyright":"imwyw ",
            "update_label":"modified at:",
            "update_format":"YYYY-MM-DD"
        }
    }
}